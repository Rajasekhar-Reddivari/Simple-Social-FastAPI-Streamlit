# Example environment variables for the FASTAPI project
# Copy this file to .env and fill with your own values before running

# ImageKit credentials
IMAGEKIT_PRIVATE_KEY=your_private_key_here
IMAGEKIT_PUBLIC_KEY=your_public_key_here
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_imagekit_id

# FastAPI Users / JWT settings (optional)
SECRET_KEY=change_me_to_a_secure_random_value
DATABASE_URL=sqlite+aiosqlite:///./test.db

# Optional: adjust host/port for uvicorn
HOST=0.0.0.0
PORT=8000


